{
  "name": "booBOT",
  "displayName": "boo!BOT",
  "description": "boo!BOT your haunted enCounter",
  "icon": "booIcon.png",
  "publisher": "booatthebot",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:booBOT.helloWorld",
    "onCommand:booBOT.getRandomFactNow",
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "booBOT.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "booBOT.getRandomFactNow",
        "title": "Get Random Fact Now"
      }
    ],
    "configuration": {
      "title": "booBOT",
      "properties": {
        "booBOT.timeInterval": {
          "type": "number",
          "default": 60,
          "description": "Edit this to change how often, in minutes, the extension alerts. Don't forget to restart your VS Code."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5"
  }
}
